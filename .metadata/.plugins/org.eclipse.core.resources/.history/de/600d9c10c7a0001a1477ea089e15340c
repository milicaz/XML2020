                <xs:element name="CDW" type="xs:boolean"/>
                <xs:element name="Decija_mesta" type="xs:int"/>
                <xs:element name="Prosecna_ocena" type="xs:float"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="CTMarkaAuta">
        <xs:sequence>
            <xs:element name="Id" type="xs:long"/>
            <xs:element name="Name" type="xs:string"/>
            <xs:element ref="k:Automobili"/>
            <xs:element ref="k:Modeli"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTModelAuta">
        <xs:sequence>
            <xs:element name="Id" type="xs:long"/>
            <xs:element name="Ime_modela" type="xs:string"/>
            <xs:element name="Marka" type="k:CTMarkaAuta"/>
        </xs:sequence>
    </xs:complexType>
