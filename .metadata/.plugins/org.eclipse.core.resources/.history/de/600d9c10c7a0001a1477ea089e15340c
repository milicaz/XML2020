            <xs:element name="Ime_klase" type="xs:string"/>
            <xs:element ref="k:Automobili"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTVrstaGoriva">
        <xs:sequence>
            <xs:element name="Id" type="xs:long"/>
            <xs:element name="Ime_goriva" type="xs:string"/>
            <xs:element ref="k:Automobili"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTTipMenjaca">
        <xs:sequence>
            <xs:element name="Id" type="xs:long"/>
            <xs:element name="Ime_menjaca" type="xs:string"/>
            <xs:element ref="k:Automobili"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="Automobili">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="k:Automobil"/>
